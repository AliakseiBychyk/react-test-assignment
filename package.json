{
  "name": "test_assignment_rtl_react",
  "version": "0.1.0",
  "private": true,
  "author": "Aliaksei Bychyk",
  "homepage": "https://aliakseibychyk.github.io/react-test-assignment",
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn sass:build && react-scripts build",
    "test": "jest --config=jest.config.json",
    "eject": "react-scripts eject",
    "sass:watch": "node-sass src/styles/styles.scss src/styles/styles.css -w",
    "sass:build": "node-sass --output-style compressed src/styles/styles.scss > src/styles/styles.css",
    "lint": "eslint ./**/*.js",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "fetch-everywhere": "^1.0.5",
    "moment": "^2.22.1",
    "node-sass": "^4.9.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "sass": "^1.3.2"
  },
  "devDependencies": {
    "enzyme-to-json": "^3.3.4",
    "eslint-config-jest-enzyme": "^6.0.1",
    "eslint-plugin-jest": "^21.17.0",
    "gh-pages": "^1.1.0",
    "jest": "^23.0.1",
    "sinon": "^5.0.10"
  }
}
